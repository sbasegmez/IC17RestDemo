<?xml version="1.0" encoding="UTF-8"?>
<xp:view
	xmlns:xp="http://www.ibm.com/xsp/core"
	xmlns:xc="http://www.ibm.com/xsp/custom"
	xmlns:xe="http://www.ibm.com/xsp/coreex">
	<xe:dynamicContent
		id="dynamicContent1"
		useHash="false"
		defaultFacet="#{box.authenticated?'yes':'no'}">
		<xp:this.facets>
			<xc:boxNotConnected
				xp:key="no"></xc:boxNotConnected>
			<xc:boxConnected
				xp:key="yes"></xc:boxConnected>
		</xp:this.facets>
	</xe:dynamicContent>
	
</xp:view>